.tltle-head {
  height: 56rpx;
  padding-left: 30rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: rgba(151, 151, 151, 1);
  line-height: 56rpx;
}

.upload-part {
  height: 279rpx;
  background: rgba(255, 255, 255, 1);
  padding: 30rpx;
  display: flex;
  justify-content: space-between;
}

.photo-left,
.photo-right {
  width: 330rpx;
  height: 220rpx;
  background: red;
}

.business-license {
  height: 543rpx;
  background: rgba(255, 255, 255, 1);
  padding: 29rpx 25rpx 0 35rpx;
}

.license-img {
  width: 244rpx;
  height: 338rpx;
  background: rebeccapurple;
  margin: 0 auto 50rpx;
}

.license-tip {
  display: flex;
  justify-content: space-between;
}

.tip-img {
  width: 32rpx;
  height: 30rpx;
  margin-right: 10rpx;
}

.tip-txt {
  font-size: 26rpx;
  font-weight: 400;
  color: rgba(151, 151, 151, 1);
  line-height: 37rpx;
}

.bottom {
  background: rgba(255, 255, 255, 1);
  padding-bottom: 98rpx;
}

.form {
  padding: 0 30rpx 60rpx;
  height: 615rpx;
}

.form-item {
  display: flex;
  height: 110rpx;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1rpx solid rgba(245, 245, 245, 1);
}

.item-left {
  width: 128rpx;
  font-size: 32rpx;
  font-weight: 400;
  color: rgba(58, 54, 54, 1);
  line-height: 45rpx;
  margin-right: 93rpx;
  text-align: justify;
}

.item-input {
  flex: 1;
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(202, 202, 202, 1);
  line-height: 40rpx;
}

.xieyi {
  margin-top: 50rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 40rpx;
}

radio .wx-radio-input {
  width: 40rpx;
  height: 40rpx;
}

.radio-txt {
  margin-left: 20rpx;
  display: flex;
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(58, 54, 54, 1);
  align-items: center;
  height: 40rpx;
}

.xieyi-txt {
  font-size: 28rpx;
  font-weight: 400;
  color: #C9AC79;
  line-height: 40rpx;
  margin-left: 15rpx;
}