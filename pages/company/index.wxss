.company-list {
  padding: 0 20px;
}

.line-item {
  height: 48px;
  padding: 16px 0;
  line-height: 24px;
}

.item-mid {
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin-left: 60px;
}

.item-name {
  font-size: 16px;
  height: 24px;
}
.name {

  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  float: left;
}
.time {
  color: #B7B7B7;
  font-size: 12px;
  height: 24px;
}



.company-icon {
  width: 48px;
  height: 48px;
}

.item-left {
  float: left;
}

.fixed-btn {
  width: 90%;
  left: 5%;
}
.img-btn {
  width: 22px;
  height: 22px;
  margin-left: 5px;
  float: right;

  margin-top: 5px;
}
.renzheng {
  border: 1px solid #327BF8;
  border-radius: 2px;
  padding: 1px 3px;
  color: #327BF8;
  font-size: 10px;
  display: inline-block;
  margin-top: 5px;
  height: 14px;
  line-height: 14px;
}
.renzheng-img {

  width: 10px;
  height: 10px;
}
.moren {
  background-color: red;
  color: #fff;
  font-size: 10px;
  padding: 0 4px;
  line-height: 16px;
  float: left;
  margin-top: 4px;
  border-radius: 2px;
  margin-left: 3px;
}
navigator {
  display: inline;
}