.collection-add {
  padding: 30rpx;
}

.card {
  width: 690rpx;
  height: 390rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 10rpx 40rpx 0px rgba(0, 0, 0, 0.05);
  border-radius: 10rpx;
  margin: 0 auto;
  padding: 20rpx;
}

.card-yinhang {
  width: 650rpx;
  height: 290rpx;
  background-size: 100% 100%;
  background-image: url(data:image/png;base64,);
}

.card-zhifubao {
  width: 650rpx;
  height: 290rpx;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
}

.head {
  display: flex;
  font-size: 30rpx;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  padding: 50rpx 0 0 70rpx;
  align-items: center;
}

.head image {
  width: 80rpx;
  height: 80rpx;
  margin-right: 18rpx;
}

.phone {
  margin-top: 31rpx;
  font-size: 36rpx;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
  padding-left: 168rpx;
}

.name {
  margin-top: 21rpx;
  font-size: 30rpx;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  padding-left: 168rpx;
}

.card-bottom {
  height: 80rpx;
  display: flex;
  align-items: center;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 24rpx;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
}

.delete {
  display: flex;
  align-items: center;
}

.del-img {
  width: 40rpx;
  height: 40rpx;
  margin-left: 3rpx;
}


.tip {
  margin: 50rpx 0 30rpx;
  padding-left: 110rpx;
}

.tip-content {
  display: flex;
  font-size: 24rpx;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  line-height: 33rpx;
}

.tip-content image {
  width: 30rpx;
  height: 30rpx;
  margin-right: 20rpx;
}

.upload {
  width: 690rpx;
  height: 120rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 10rpx 40rpx 0px rgba(0, 0, 0, 0.05);
  border-radius: 10rpx;
  display: flex;
  align-items: center;
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(201, 172, 121, 1);
  justify-content: center;
}

.upload image {
  width: 36rpx;
  height: 36rpx;
  margin-right: 10rpx;
}